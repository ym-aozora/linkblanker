{
	"parser": "babel-eslint",

	"envs": {
		"brower": true,
		"node": true,
		"es6": true
	},

	"plugins": [
		"react"
	],

	"ecmaFeatures": {
		"modules": true,
		"jsx": true
	},

	"globals": {
		"console": false,
		"window": false,
		"chrome": false,
		"require": false,
		"localStorage": false,
		"sessionStorage": false,
		"setInterval": false,
		"setTimeout": false,
		"createjs": false,
		"clearInterval": false,
		"clearTimeout": false,
		"module": false,
		"XMLHttpRequest": false,
		"__NODE_ENV__": false
	},

	"extends": "eslint:recommended",
	"rules": {
		"no-unused-vars": 0
	}
}
