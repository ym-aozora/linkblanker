{
	"manifest_version": 2,
	"version": "2.3.1",
	"name": "Link Blanker",
	"description": "Open link in new tab.",
	"icons": {
		"128": "dest/images/icon.png",
		"16": "dest/images/icon16.png",
		"19": "dest/images/icon19.png",
		"32": "dest/images/icon32.png",
		"48": "dest/images/icon48.png",
		"64": "dest/images/icon64.png"
	},
	"permissions": [
		"background",
		"tabs"
	],
	"web_accessible_resources": [
		"dest/images/icon48.png"
	],
	"background": {
		"scripts": [
			"dest/javascripts/bundle-background.min.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"css": ["dest/stylesheets/contentscript.css"],
			"js": [
				"vendor/easeljs-NEXT.min.js",
				"vendor/tweenjs-NEXT.min.js",
				"dest/javascripts/bundle-contentscript.min.js"
			],
			"run_at": "document_start"
		}
	],
	"browser_action": {
		"default_icon": {
			"19": "dest/images/icon19.png",
			"32": "dest/images/icon32.png"
		},
		"default_title": "Link Blanker",
		"default_popup": "dest/html/popup.html"
	},
	"default_locale":"en"
}
