{
	"manifest_version": 2,
	"version": "1.0.7",
	"name": "Link Blanker",
	"description": "Open link in new tab.",
	"icons": {
		"128": "images/icon.png",
		"16": "images/icon16.png",
		"19": "images/icon19.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"64": "images/icon64.png"
	},

	"permissions": [
		"background",
		"tabs"
	],
	"web_accessible_resources": [
		"javascripts/jquery-2.0.3.min.map",
		"images/close-action.png"
	],
	"background": {
		"scripts": [
			"javascripts/jquery-2.0.3.min.js",
			"javascripts/background.js"
		]
	},
    "content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"css": ["stylesheets/content-script.css"],
			"js": ["javascripts/jquery-2.0.3.min.js", "javascripts/contentscript.js"],
			"run_at": "document_end"
		}
	],
	"browser_action": {
		"default_icon": {
			"19": "images/icon19.png",
			"32": "images/icon32.png"
		},
		"default_title": "Link Blanker",
		"default_popup": "popup.html"
	},
	"default_locale":"en"
}
