{
	"manifest_version": 2,
	"version": "2.4.0",
	"name": "Link Blanker",
	"description": "Open link in new tab.",
  "homepage_url": "https://www.aozora-create.com/service/linkblanker",
	"icons": {
		"128": "img/icon.png",
		"16": "img/icon16.png",
		"19": "img/icon19.png",
		"32": "img/icon32.png",
		"48": "img/icon48.png",
		"64": "img/icon64.png"
	},
	"permissions": [
		"background",
		"tabs"
	],
	"web_accessible_resources": [
    "img/icon-disabled.svgz",
		"img/icon-enabled.svgz"
	],
	"background": {
		"scripts": [
			"js/bundle.background.min.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"css": ["css/contentscript.min.css"],
			"js": [
				"vendor/easeljs-NEXT.min.js",
				"vendor/tweenjs-NEXT.min.js",
				"js/bundle.contentscript.min.js"
			],
			"run_at": "document_start"
		}
	],
	"browser_action": {
		"default_icon": {
			"19": "img/icon19.png",
			"32": "img/icon32.png"
		},
		"default_title": "Link Blanker",
		"default_popup": "html/popup.html"
	},
	"default_locale":"en"
}
