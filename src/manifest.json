{
	"manifest_version": 2,
	"version": "2.4.0",
	"name": "Link Blanker",
	"description": "Open link in new tab.",
	"icons": {
		"128": "img/icon.png",
		"16": "img/icon16.png",
		"19": "img/icon19.png",
		"32": "img/icon32.png",
		"48": "img/icon48.png",
		"64": "img/icon64.png"
	},
	"permissions": [
		"background",
		"tabs"
	],
	"web_accessible_resources": [
		"img/icon48.png"
	],
	"background": {
		"scripts": [
			"js/bundle.linkblanker.min.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*", "file:///*"],
			"css": ["css/agent.min.css"],
			"js": [
				"js/bundle.agent.min.js"
			],
			"run_at": "document_start"
		}
	],
	"browser_action": {
		"default_icon": {
			"19": "img/icon19.png",
			"32": "img/icon32.png"
		},
		"default_title": "Link Blanker",
		"default_popup": "index.html#/popup"
	},
  "options_page": "index.html#/tree",
	"default_locale":"en"
}
